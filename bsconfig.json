{
  "name": "bs-dom-testing-library",
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-super-errors"
  ],
  "reason": {
    "react-jsx": 2
  },
  "refmt": 3,
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        {
          "dir": "__tests__",
          "type": "dev"
        }
      ]
    }
  ],
  "bs-dev-dependencies": [
    "bs-jest"
  ]
}
